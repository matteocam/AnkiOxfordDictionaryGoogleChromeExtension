{
  "update_url":"http://clients2.google.com/service/update2/crx",
  "name": "AnkiOxford",
  "version": "0.1",
  "description": "Add any English word and its definition from Oxford dictionary to your Anki",
  "icons": {
	"16": "icons/16.png",
	"32": "icons/32.png",
	"48": "icons/48.png",
	"128": "icons/128.png"
  },
  "permissions": [
	"tabs",
	"http://*/*",
	"https://*/*",
	"contextMenus"
  ],
  "content_scripts": [ {
	  "js": [ "js/startup.js" ],
	  "matches": [ "https://*/*", "http://*/*" ],
	  "run_at": "document_end"
   } ],
   "browser_action": {
	  "default_icon": "icons/32.png",
	  "default_title": "AnkiOxford"
   },
  "background_page": "background.html",
  "options_page": "options.html"
}
